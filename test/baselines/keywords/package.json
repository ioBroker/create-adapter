{
  "name": "iobroker.test-adapter",
  "version": "0.0.1",
  "description": "test-adapter",
  "author": {
    "name": "Al Calzone",
    "email": "al@calzo.ne"
  },
  "homepage": "https://github.com/AlCalzone/ioBroker.test-adapter",
  "license": "MIT",
  "keywords": [
    "this",
    "adapter",
    "uses",
    "different",
    "keywords"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlCalzone/ioBroker.test-adapter"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.4.0"
  },
  "devDependencies": {
    "@iobroker/testing": "^2.3.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/gulp": "^4.0.7",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.5",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "axios": "^0.21.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.12.1",
    "gulp": "^4.0.2",
    "mocha": "^8.2.0",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts src/"
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/ioBroker.test-adapter/issues"
  },
  "readmeFilename": "README.md"
}
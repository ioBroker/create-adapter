{
  "name": "iobroker.test-adapter",
  "version": "0.0.1",
  "description": "test-adapter",
  "author": {
    "name": "Al Calzone",
    "email": "al@calzo.ne"
  },
  "homepage": "https://github.com/AlCalzone/ioBroker.test-adapter",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "template",
    "Smart Home",
    "home automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlCalzone/ioBroker.test-adapter"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@iobroker/adapter-react": "^1.6.1",
    "@iobroker/testing": "^2.4.2",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/gulp": "^4.0.8",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.28",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^16.9.11",
    "@types/react": "^16.14.4",
    "@types/sinon": "^9.0.10",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "axios": "^0.21.1",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "gulp": "^4.0.2",
    "mocha": "^8.3.0",
    "parcel-bundler": "^1.12.4",
    "proxyquire": "^2.1.3",
    "react-dom": "^16.14.0",
    "react-icons": "^4.2.0",
    "react": "^16.14.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx -d admin/build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/"
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/ioBroker.test-adapter/issues"
  },
  "readmeFilename": "README.md"
}